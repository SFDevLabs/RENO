  "devDependencies": {
    "babel-loader": "^5.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "jest-cli": "^0.8.1",
    "nodemon": "*",
    "supertest": "1.0.1",
    "should": "7.0.1",
    "mocha": "2.2.5"
